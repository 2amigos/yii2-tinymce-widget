{
    "name": "2amigos/yii2-tinymce-widget",
    "description": "TinyMCE widget for Yii2.",
    "keywords": [
        "yii",
        "extension",
        "widget",
        "tinymce"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "tinymce/tinymce": ">= 4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\tinymce\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dosamigos\\tinymce\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
